{
	"name": "css-variables-hints",
	"displayName": "css-variables-hints",
	"publisher": "airbender92",
	"description": "",
	"version": "1.0.1",
	"icon": "img/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/airbender92/css-variables-hints"
	},
	"engines": {
		"vscode": "^1.56.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:css",
		"onLanguage:scss",
		"onLanguage:less",
		"onLanguage:postcss",
		"onLanguage:vue"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "CSS Variables hints",
			"properties": {
				"cssVarriablesHints.files": {
					"type": "array",
					"description": "Array of paths to files with css variables"
				},
				"cssVarriablesHints.remoteFiles": {
					"type": "array",
					"description": "Array of paths to remotefiles with css variables"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/css": "0.0.31",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/strip-comments": "^2.0.0",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"css": "^3.0.0",
		"scss-parser": "^1.0.4",
		"strip-comments": "^2.0.1"
	}
}
